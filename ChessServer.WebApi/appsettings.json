{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.File",
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "../logs/apilog-.log",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "ConnectionStrings": {
    "psqlConnection": ""
  },
  "JwtTokenSettings": {
    "SecretKey": "",
    "Issuer": "ChessServer",
    "Audience": "ChessServer",
    "ExpiresInDays": 7
  }
}